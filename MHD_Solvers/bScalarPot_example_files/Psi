FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/magnets";
    object      Psi;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 0 0 0 0 1 0 ];

internalField   uniform 0;

boundaryField
{
    magnets_to_air
    {
        type            psiCoupleConstMur;
        value           uniform 0;
        psinbr          Psi;
    }
    magnets_to_iron
    {
        type            psiCoupleConstMur;
        value           uniform 0;
        psinbr          Psi;
    }
    magnets_to_liquid
    {
        type            psiCoupleConstMur;
        value           uniform 0;
        psinbr          Psi;
    }
}


