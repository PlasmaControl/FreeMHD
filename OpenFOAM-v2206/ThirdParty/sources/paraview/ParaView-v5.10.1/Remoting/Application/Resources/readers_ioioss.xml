        number_of_elements="1"
        default_values="1"
        panel_visibility="advanced">
        <BooleanDomain name="bool" />
        <Documentation>
          When checked, the reader will read quality assurance and
          information records, if available.
        </Documentation>
      </IntVectorProperty>

