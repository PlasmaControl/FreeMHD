FoamFile
{
    version   2.0;
    format    ascii;
    class     dictionary;
    object    controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application         pMesh;

startFrom           latestTime;

startTime           0;

stopAt              endTime;

endTime             10;

deltaT              0.05;

writeControl        timeStep;

writeInterval       20;

purgeWrite          0;

writeFormat         ascii;

writePrecision      6;

runTimeModifiable yes;

