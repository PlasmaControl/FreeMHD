    -g -ggdb -DFULLDEBUG -O0 \
    -I$(LIB_SRC)/fileFormats/lnInclude \
    -I$(LIB_SRC)/surfMesh/lnInclude \
    -I$(LIB_SRC)/meshTools/lnInclude \
    -I../../meshLibrary/lnInclude

EXE_LIBS = \
    -lmeshTools \
    -L$(FOAM_MODULE_LIBBIN) -lmeshLibrary
