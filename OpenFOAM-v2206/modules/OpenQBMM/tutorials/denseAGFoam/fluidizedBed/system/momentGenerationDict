phases
(
    particles
);

boundaries
{
    inlet
    {
        type    fixedValue;
        value   uniform 0;
    }
    outlet
    {
        type    zeroGradient;
    }
    walls
    {
        type    zeroGradient;
    }
    frontAndBackPlanes
    {
        type    empty;
    }
}

particles
{
    type                alphaAndDiameter;
    Radau               false;
    weightDimension     [0 -3 0 0 0 0 0];
    abscissaDimension   [1 0 0 0 0 0 0];

    internal
    {
        node0
        {
            rho rho [1 -3 0 0 0 0 0] 2500;
            dia dia [0 1 0 0 0 0 0] 3e-4;
            alpha alpha [0 0 0 0 0 0 0] 0.55;
        }
        node1
        {
            rho rho [1 -3 0 0 0 0 0] 2500;
            dia dia [0 1 0 0 0 0 0] 6e-4;
            alpha alpha [0 0 0 0 0 0 0] 0.55;
        }
    }
    inlet
    {
        node0
        {
            rho rho [1 -3 0 0 0 0 0] 2500;
            dia dia [0 1 0 0 0 0 0] 3e-4;
            alpha alpha [0 0 0 0 0 0 0] 0;
        }
    }
}


