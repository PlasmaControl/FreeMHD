FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

regions
(
    boxToCell
    {
        box ( 0 0.4 -0.1 ) ( 0.5 2 0.1 );
        fieldValues
        (
            volScalarFieldValue alpha.particles     0
        );
    }
);

