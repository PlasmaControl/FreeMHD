FoamFile
{
    version     2.0;
    format      ascii;
    class       volSymmTensorField;
    object      R;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform (0.01 0.01 0.01 0.01 0.01 0.01);

boundaryField
{
    "wedge.*"
    {
        type            wedge;
    }
    innerWall
    {
        type            kqRWallFunction;
        value           $internalField;
    }
    outerWall
    {
        type            kqRWallFunction;
        value           $internalField;
    }
    inlet
    {
        type            kqRWallFunction;
        value           $internalField;
    }
    outlet
    {
        type            kqRWallFunction;
        value           $internalField;
    }
}

