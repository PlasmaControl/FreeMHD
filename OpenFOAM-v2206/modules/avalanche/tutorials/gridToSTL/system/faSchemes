FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      faSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    ddt(hentrain) backward;
    ddt(h,Us) backward;
    ddt(h) backward;
}

gradSchemes
{
    default Gauss linear;
}

divSchemes
{
    default             none;
    div(phis,h)         Gauss upwind;
    div(phi2s,Us)       Gauss upwind;
}

laplacianSchemes
{
    default none;
    laplacian(h) Gauss linear corrected;
}

interpolationSchemes
{
    default linear;
}

snGradSchemes
{
    default corrected;
}

fluxRequired
{
    h;
}

