FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      meshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

maxCellSize 40;

surfaceFile "constant/surface.stl";

localRefinement
{
    terrain
    {
        additionalRefinementLevels 2;
    }
}

renameBoundary
{
    newPatchNames
    {
        "wall.*"
        {
            newName wall;
            type patch;
        }
    }
}

