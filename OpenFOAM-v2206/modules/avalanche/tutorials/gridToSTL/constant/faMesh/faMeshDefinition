FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      faMeshDefinition;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

polyMeshPatches  1( terrain );

boundary
{
    wall
    {
        type                patch;
        ownerPolyPatch      terrain;
        neighbourPolyPatch  wall;
    }
}

