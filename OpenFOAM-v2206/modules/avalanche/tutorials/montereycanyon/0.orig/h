FoamFile
{
    version     2.0;
    format      ascii;
    class       areaScalarField;
    location    "0";
    object      h;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   uniform 1e-5;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 150;
    }
    outlets
    {
        type            zeroGradient;
        value           $internalField;
    }
    walls
    {
        type            fixedValue;
        value           uniform 0;
    }
}


