FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application         faSavageHutterFoam;

startFrom           latestTime;

startTime           0;

stopAt              endTime;

endTime             0.05;

deltaT              0.01;

writeControl        adjustableRunTime;

writeInterval       0.01;

purgeWrite          0;

writeFormat         ascii;

writePrecision      6;

writeCompression    off;

timeFormat          general;

timePrecision       6;

runTimeModifiable   yes;

adjustTimeStep      yes;

initDeltaT          yes;

maxCo               1.0;

