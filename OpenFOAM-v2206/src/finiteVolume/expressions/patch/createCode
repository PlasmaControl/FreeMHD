#!/bin/sh
cd "${0%/*}" || exit                                # Run from this directory
#------------------------------------------------------------------------------
# Manually create ragel scanner and lemon parser header

prefix=patchExpr

"${WM_PROJECT_DIR:?}/wmake/scripts/makeParser" \
    -prefix="$prefix" \
    -scanner=Scanner.rl \
    -parser=LemonParser.lyy-m4 \
    "$@"

#------------------------------------------------------------------------------
